#include <stdio.h>
#include <string.h>

int main(void) {
  int n;
  int count = 0;
  scanf("%d",&n);
  while(n != 0)
  {
    count ++;
    n = n/10;
  }
  if(count == 1)
  {
    printf("1");
  }
  else if(count == 2)
  {
    printf("2");
  }
  else if(count == 3)
  {
    printf("3");
  }
  else
  {
    printf("More than 3 digits");
  }
  return 0;
}
